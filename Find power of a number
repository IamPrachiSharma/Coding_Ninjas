import java.util.* ;

import java.io.*; 
class Solution {
	
	public static void main(String args[]) {
		
		// Write code here
		Scanner sc= new Scanner(System.in);
		int x = sc.nextInt();
		int n = sc.nextInt();

		int p= 1;

		if(x==0 && n==0){
			System.out.println("1");
		}else{
			for(int i=0;i<n;i++){
				p =x*p;
			}
			System.out.println(p);
		}
		
	}
}
