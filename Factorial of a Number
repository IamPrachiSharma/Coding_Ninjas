import java.util.Scanner;
class Solution {
	
	public static void main(String args[]) {
		
		// Write code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int p =n;

		if(n==0){
			System.out.println("1");
		}else if(n>=1){
			for(int i=1;i<n;i++){
				p=p*i;
			}
			System.out.println(p);
		}else{
			System.out.println("Error");
		}
		
	}
}
